@model VMDCore.Models.AdminViewModel
@{ ViewData["Title"] = "Index"; }

@{await Html.RenderPartialAsync("_ViewAllDrink", @Model.Drinks);}

@{await Html.RenderPartialAsync("_ViewAllCoin", @Model.Coins);}

<div class="modal fade" id="coinModal" tabindex="-1" role="dialog" aria-labelledby="coinModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="coinModalLabel"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
               
            </div>
        </div>
    </div>
</div>




@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}